function doHomework(subject, callback) {
  alert(`I love my ${subject} sooo much!!!!`);
  callback();
}
function alertFinished(){
  alert('Cannot wait to see you tonight!!!!');
}
doHomework('Tiffy', alertFinished);

Additions
function counter (arr, add) {
newArr = [];
	for (let i = 0; i < arr.length; i++) {
		newArr.push(arr[i] + add);
		}
	return newArr;
	}
console.log(counter([5,0,4], 5));


function counter (arr, add) {
 return arr.map(a => a + add);
	}
console.log(counter([5,0,4], 10));
